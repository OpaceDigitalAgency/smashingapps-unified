{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:seo-module": "npx esbuild src/utils/seoMaster.cjs.ts --bundle --platform=node --format=cjs --outfile=dist/utils/seoMaster.cjs.js",
    "build": "npx tsc && npm run build:seo-module && npx vite build && node tools/task-smasher/scripts/generate-static-pages.js && node scripts/generate-seo-files.js",
    "generate-seo": "npm run build:seo-module && node scripts/generate-seo-files.js",
    "generate-og": "node scripts/generate-og-images.js",
    "generate-favicons": "node scripts/generate-favicons.js",
    "inject-meta": "npm run build:seo-module && node scripts/inject-meta-tags.cjs",
    "prerender": "vite preview --port 5173 & npm run build:seo-module && node scripts/prerender-routes.js && pkill -f 'vite preview'",
    "build:seo": "npm run build && npm run inject-meta",
    "cleanup-seo": "node scripts/cleanup-old-seo-files.js",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@netlify/functions": "^3.0.4",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "axios": "^1.8.4",
    "busboy": "^1.6.0",
    "lucide-react": "^0.344.0",
    "openai": "^4.91.1",
    "postcss": "^8.4.35",
    "puppeteer": "^22.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.4.1",
    "serve": "^14.2.1",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "typescript-eslint": "^8.3.0"
  }
}
